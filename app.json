{
  "expo": {
    "name": "Cheza Pro",
    "slug": "cheza-pro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "sports",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#11111b"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sports.tracker"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#11111b"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.sports.tracker"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "01fb33eb-9dc2-4d9e-802d-f7f25a96e9b4"
      }
    },
    "owner": "mainaport",
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
